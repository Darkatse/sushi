{
    "version": "202006282209",
    "description": "Enhanced ruleset for V2Ray",
    "homepage": "https://github.com/Loyalsoldier/v2ray-rules-dat",
    "license": "GPL-3.0-or-later",
    "url": "https://github.com/Loyalsoldier/v2ray-rules-dat/releases/download/202006282209/rules.zip",
    "hash": "36f2173a452a41eef5467ab8ce812e8d7afcdfe6bd4daf07b9dfb3c26a49c0d0",
    "depends": "v2ray",
    "installer": {
        "script": [
            "function LinkFile([String] $file, [String] $source = $file) {",
            "    Write-Host \"Creating hardlink for $file\"",
            "    New-Item -Force -ItemType HardLink -Path \"$(appdir v2ray $global)\\current\" -Name $file -Target \"$dir\\$source\" | Out-Null",
            "}",
            "LinkFile 'geoip.dat'",
            "LinkFile 'geosite.dat'"
        ]
    },
    "uninstaller": {
        "script": [
            "Remove-Item \"$(appdir v2ray $global)\\current\\geoip.dat\" -Force",
            "Remove-Item \"$(appdir v2ray $global)\\current\\geosite.dat\" -Force"
        ]
    },
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/Loyalsoldier/v2ray-rules-dat/releases/download/$version/rules.zip"
    }
}
