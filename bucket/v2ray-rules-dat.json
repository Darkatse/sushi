    "post_install": [
        "Write-Host \"Creating hardlink for geoip.dat\"",
        "New-Item -Force -ItemType HardLink -Path \"$(appdir v2ray $global)\\current\" -Name geoip.dat -Target \"$dir\\geoip.dat\" | Out-Null",
        "Write-Host \"Creating hardlink for geosite.dat\"",
        "New-Item -Force -ItemType HardLink -Path \"$(appdir v2ray $global)\\current\" -Name geosite.dat -Target \"$dir\\geosite.dat\" | Out-Null"
    ],
    "uninstaller": {
        "script": [
            "Remove-Item \"$(appdir v2ray $global)\\current\\geoip.dat\" -Force",
            "Remove-Item \"$(appdir v2ray $global)\\current\\geosite.dat\" -Force"
        ]
    },
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/Loyalsoldier/v2ray-rules-dat/releases/download/$version/rules.zip"
    }
