    "architecture": {
        "64bit": {
            "url": "https://vscode.cdn.azure.cn/insider/c828a5339bca7222fd8f56ab98361a98f736d185/VSCode-win32-x64-1.48.0-insider.zip",
            "hash": "050e9adc295c57aa3554a7cbd6cc03c621c103f99440bf603753d7cc12fed1f8"
        },
        "32bit": {
