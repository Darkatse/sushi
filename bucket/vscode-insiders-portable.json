            "hash": "8f67b2994173cfd56c2fed601b5fb1644acbba632b5859111ef6dc84f397e572"
        }
    },
    "bin": [["bin\\code-insiders.cmd", "code-insiders"]],
    "shortcuts": [["Code - Insiders.exe", "Visual Studio Code - Insiders"]],
    "persist": "data",
    "checkver": {
        "url": "https://scoop-sushi.vercel.app/api/vscode-insiders.ts",
        "regex": "(?<version>[\\d.]+) (?<commit>(?<shortCommit>.{7}).+)",
        "replace": "${version}-${shortCommit}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://vscode.cdn.azure.cn/insider/$matchCommit/VSCode-win32-x64-$matchHead-insider.zip",
                "hash": {
                    "url": "https://code.visualstudio.com/sha?build=insider",
                    "jsonpath": "$.products[5].sha256hash"
                }
            },
            "32bit": {
                "url": "https://vscode.cdn.azure.cn/insider/$matchCommit/VSCode-win32-ia32-$matchHead-insider.zip",
                "hash": {
                    "url": "https://code.visualstudio.com/sha?build=insider",
                    "jsonpath": "$.products[2].sha256hash"
                }
            }
        }
    }
}
