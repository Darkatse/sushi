            "Visual Studio Code - Insiders"
        ]
    ],
    "persist": "data",
    "checkver": {
        "url": "https://scoop-sushi.vercel.app/api/vscode-insiders.ts",
        "regex": "(?<version>[\\d.]+) (?<commit>(?<shortCommit>.{7}).+)",
        "replace": "${version}-${shortCommit}"
